server {
    listen 8091;
    server_name 10.253.254.74;

    location /HylaWEB {
        include proxy_params;
        proxy_set_header X-Script-Name /HylaWEB;
        proxy_pass http://unix:/opt/HylaWEB/API/HylaWEBapi.sock;
    }
}

server {
    listen 80;
    server_name 10.253.254.74;

    location /HylaWEB {
        include proxy_params;
        proxy_set_header X-Script-Name /HylaWEB;
        proxy_pass http://unix:/opt/HylaWEB/WEB/HylaWEB.sock;
    }

    location  /HylaWEB/static {
        alias /opt/HylaWEB/WEB/static;
        autoindex on;
        expires 1d;
    }

}

